<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.fz.travel.bean.LineScenery" table="lineScenery" catalog="travel">
        <id name="lineSceneryId" type="java.lang.Integer">
            <column name="LINESCENERYID" />
            <generator class="native" />
        </id>
        <property name="order" type="java.lang.Integer">
            <column name="ORDER" />
        </property>
        <many-to-one name="scenery" class = "com.fz.travel.bean.Scenery" column="sceneryId" />
        <many-to-one name="touristLine" class="com.fz.travel.bean.TouristLine" column="touristLineId" />
    </class>
</hibernate-mapping>